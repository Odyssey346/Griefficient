{
	"name": "griefficient",
	"version": "1.1.0",
	"description": "A tool that scans Shodan for Minecraft servers and gives you information about them, to help you mess around with public Minecraft servers.",
	"scripts": {
		"dev": "tsup --watch",
		"preview": "node dist/index.js",
		"build": "tsup",
		"local": "sudo npm i -g && griefficient",
		"refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
		"format": "prettier --write ."
	},
	"bin": {
		"griefficient": "./dist/index.js"
	},
	"author": "Odyssey346",
	"keywords": [
		"minecraft",
		"shodan",
		"gamedig"
	],
	"license": "MIT",
	"homepage": "https://github.com/Odyssey346/Griefficient#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Odyssey346/Griefficient.git"
	},
	"bugs": {
		"url": "https://github.com/Odyssey346/Griefficient/issues"
	},
	"dependencies": {
		"@hapi/bourne": "^3.0.0",
		"chalk": "^5.0.1",
		"commander": "^9.3.0",
		"got": "^12.1.0"
	},
	"devDependencies": {
		"prettier": "2.7.1",
		"tsup": "^6.2.1"
	},
	"type": "module"
}
