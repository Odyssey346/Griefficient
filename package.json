{
  "name": "griefficient",
  "version": "1.0.0",
  "description": "A tool that scans Shodan for Minecraft servers and gives you information about them, to help you mess around with public Minecraft servers.",
  "main": "./lib/index.js",
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node --esm' src/index.ts",
    "start:windows": "nodemon --watch 'src/**/*.ts' --exec \"npx ts-node\" src/index.ts",
    "create": "npm run build && npm run start",
    "build": "tsc -p .",
    "local": "sudo npm i -g && griefficient",
    "refresh": "rm -rf ./node_modules ./package-lock.json ./pnpm-lock.yaml && npm install"
  },
  "bin": {
    "griefficient": "./lib/index.js"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Odyssey346/Griefficient.git"
  },
  "keywords": [
    "minecraft",
    "shodan",
    "gamedig"
  ],
  "author": "Odyssey346",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Odyssey346/Griefficient/issues"
  },
  "homepage": "https://github.com/Odyssey346/Griefficient#readme",
  "dependencies": {
    "@hapi/bourne": "^3.0.0",
    "chalk": "^5.0.1",
    "commander": "^9.3.0",
    "got": "^12.1.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.3",
    "nodemon": "^2.0.19",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  }
}
